{
  "navigationFallback": {
    "rewrite": "/index",
    "exclude": ["/images/*", "/*.{png,jpg,gif,ico,svg}", "/_next/*", "/static/*"]
  },
  "platform": {
    "apiRuntime": "node:20"
  },
  "routes": [
    {
      "route": "/_next/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "trailingSlash": "auto"
} 