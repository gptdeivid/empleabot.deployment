{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_next/*", "/static/*", "/images/*", "/*.{png,jpg,gif,ico,svg}", "/out/*"]
  },
  "platform": {
    "apiRuntime": "node:20"
  },
  "routes": [
    {
      "route": "/_next/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/out/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
} 