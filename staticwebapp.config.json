{
  "navigationFallback": {
    "rewrite": "/_next/static/chunks/pages/index.html",
    "exclude": ["/_next/*", "/static/*", "/images/*", "/*.{png,jpg,gif,ico,svg}"]
  },
  "platform": {
    "apiRuntime": "node:20"
  },
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "rewrite": "/api/index"
    },
    {
      "route": "/_next/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
} 